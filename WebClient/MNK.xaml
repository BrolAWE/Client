<Page x:Class="WebClient.MNK"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WebClient"
      mc:Ignorable="d" 
      d:DesignHeight="395" d:DesignWidth="744.9"
      Title="MNK">

    <Grid>
        <DataGrid x:Name="View" HorizontalAlignment="Left" Height="356" VerticalAlignment="Top" Width="140" CanUserSortColumns="False" CanUserResizeColumns="False" CanUserReorderColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{x:Null}" ClipboardContentBinding="{x:Null}" Header="X"/>
                <DataGridTextColumn Binding="{x:Null}" ClipboardContentBinding="{x:Null}" Header="Y"/>
            </DataGrid.Columns>
        </DataGrid>
        <Button Content="View" HorizontalAlignment="Left" Margin="65,361,0,0" VerticalAlignment="Top" Width="75" Height="24" Click="MNK_Click"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="0,362,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="60"/>
    </Grid>
</Page>
